<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<title>Dang ki</title>
<link rel="stylesheet" type="text/css"
	href="/FinalWeb09/clients/css/dangki.css">
</head>
<body>
	<form action="/FinalWeb09/dangki" method="post">
		<div class="all">
			<div class="logo">
				<h1>SIGN UP</h1>
			</div>
			<!-- logo -->
			<div class="input">
				<div class="name">
					<input id="n1" type="text" name="name" placeholder="Name">
				</div>
				<div class="date">
					<input id="n1" type="text" name="date" placeholder="Age">
				</div>
				<div class="gender">
					<label id="l1">Gender</label> <input id="i1" type="radio"
						checked="checked" name="gender" value="Nam">M <input
						id="i2" type="radio" name="gender" value="Nu">FM <input
						id="i3" type="radio" name="gender" value="Khac">D
				</div>
				<div class="role">
					<label id="l2">Role</label> <select id="n1" name="role">
						<option value="0">role select</option>
						<option value="1">admin</option>
						<option value="2">member</option>
					</select>
				</div>
				<div class="address">
					<label id="l2">Address</label> <select id="n1" name="address">
						<option value="0">Tinh/Thanh Pho</option>
						<option value="Ha Noi">Ha Noi</option>
						<option value="Hung Yen">Hung Yen</option>
						<option value="Hai Phong">Hai Phong</option>
						<option value="Hai Duong">Hai Duong</option>
						<option value="Ha Nam">Ha Nam</option>
					</select>
				</div>
				<div class="username">
					<input id="n1" type="text" name="userName" placeholder="UserName">
				</div>
				<div class="password">
					<input id="n1" type="password" name="passWord"
						placeholder="PassWord">
				</div>

			</div>
			<!--in put-->
			<div class="btn">
				<button id="bt1" type="submit">SIGN UP</button>
			</div>
			<!--submit-->
		</div>
	</form>
</body>
</html>