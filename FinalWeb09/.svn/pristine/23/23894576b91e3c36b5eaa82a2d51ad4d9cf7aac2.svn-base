<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<link rel="stylesheet" type="text/css"
	href="/FinalWeb09/admins/css/user/add.css">
<meta charset="UTF-8">
<title>ADD</title>
</head>
<body>
	<form action="/FinalWeb09/admin/user/add" method="post"
		enctype="multipart/form-data">


		<div class="all">
			<div class="head">
				<h1>Them Nguoi Dung</h1>
			</div>
			<div class="main">
				<div class="name">
					<h2>Name</h2>
					<input type="text" name="name" placeholder="ho ten nguoi dung">
				</div>
				<div class="age">
					<h2>Age</h2>
					<input type="text" name="age" placeholder="nhap tuoi nguoi dung">
				</div>
				<div class="gender">
					<h2>Gender</h2>
					<div class="i">
						<input type="radio" checked="checked" name="gender" value="Nam">M
						<input type="radio" name="gender" value="Nu">FM <input
							type="radio" name="gender" value="Khac">D
					</div>
				</div>
				<div class="role">
					<h2>Role</h2>
					<select name="role">
						<option value="member">member</option>
						<option value="admin">admin</option>
					</select>
				</div>
				<div class="address">
					<h2>Address</h2>
					<select name="address">
						<option value="Ha Noi">Ha Noi</option>
						<option value="Hung Yen">Hung Yen</option>
						<option value="Hai Phong">Hai Phong</option>
						<option value="Hai Duong">Hai Duong</option>
						<option value="Ha Nam">Ha Nam</option>
						<option value="Nam Dinh">Nam Dinh</option>
						<option value="Bac Ninh">Bac Ninh</option>
						<option value="Tp.Ho Chi Minh">Tp.Ho Chi Minh</option>
					</select>
				</div>
				<div class="username">
					<h2>UserName</h2>
					<input type="text" name="userName" placeholder="nhap ten dang nhap">
				</div>
				<div class="password">
					<h2>PassWord</h2>
					<input type="password" name="matKhau"
						placeholder="nhap mat khau nguoi dung">
				</div>
				<div class="password">
					<h2>ImageUrl</h2>
					<input type="file" name="imageFile" accept="image/*"
						placeholder="nhap Link anh nguoi dung">
				</div>

				<div class="btn">
					<button type="submit">ADD</button>
				</div>
			</div>
		</div>
	</form>
</body>
</html>