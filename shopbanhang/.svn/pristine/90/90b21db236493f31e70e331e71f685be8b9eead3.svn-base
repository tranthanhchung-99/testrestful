<!DOCTYPE html>
<html layout:decorator="admin/template">
<head>
<meta charset="UTF-8">
<title>thuongHieu Update</title>
</head>
<body layout:fragment="content" th:remove="tag">
	<div>
		<form th:action="@{/admin/thuongHieu/update}" method="post"
			th:modelattribute="${thuongHieu}">
			<div>
				<label>id</label><input type="text" name="id" th:value="${thuonghieu.id}" />
			</div>
			<div>
				<label>name</label><input type="text" name="name"
					th:value="${thuonghieu.name}" />
			</div>
			<div>
				<input type="submit" value="update" />
			</div>
		</form>
	</div>
</body>
</html>